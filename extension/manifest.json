{
  "manifest_version": 3,
  "name": "Daily Dispo Deals – Deal Checker",
  "version": "1.0.0",
  "description": "Overlay Worth It / Mid / Taxed badges on dispensary menu product cards.",
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Daily Dispo Deals – Analyze dispensary menu",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "permissions": ["activeTab", "scripting", "storage", "webNavigation"],
  "optional_host_permissions": [
    "*://*.weedmaps.com/*",
    "*://weedmaps.com/*",
    "*://*.dutchie.com/*",
    "*://dutchie.com/*",
    "*://mindrightmi.com/*",
    "*://*.mindrightmi.com/*",
    "*://dailydispodeals.com/*","https://dailydispodeals.com/*","http://localhost/*","http://127.0.0.1/*"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["utils/domains.js", "utils/parse.js", "utils/scoring.js", "utils/id.js", "content.js", "stub.js", "v2/config.js", "v2/installId.js", "v2/mapToIngestPayload.js", "v2/clientRef.js", "v2/overlayStore.js", "icons/16.png", "icons/32.png", "icons/48.png", "icons/128.png", "icons/logo.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
